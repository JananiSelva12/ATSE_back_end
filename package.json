{
  "name": "hk_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/server.ts",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -c hk_db -n",
    "migration:run": "npm run typeorm -- migration:run -c hk_db",
    "migration:revert": "npm run typeorm -- migration:revert -c hk",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
